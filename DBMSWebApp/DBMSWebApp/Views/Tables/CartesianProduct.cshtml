@model DBMSWebApp.Models.Table
@{
    ViewData["Title"] = "Cartesian";
}

<h2>@Model.Name</h2>

<table class="table table-responsive table-hover">
    <tr>

        @foreach (var d in Model.Columns)
        {
            <th>@d.Name</th>
        }
    </tr>
    @foreach (var d in Model.Rows)
    {
        <tr>
            @foreach (var cell in d.Cells)
            {
                <td>@cell.Value</td>
            }

        </tr>

    }
</table>
<div>
    <a asp-action="Index" asp-controller="Tables" asp-route-databaseId="@Model.DatabaseId">Back to List</a>
</div>
